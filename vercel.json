{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app/main.py"
      },
      {
        "src": "/auth/(.*)",  
        "dest": "app/main.py"
      },
      {
        "src": "/my-characters/(.*)",
        "dest": "app/main.py"
      },
      {
        "src": "/",         
        "dest": "app/main.py"
      }
    ]
  }
